// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model users{
  id Int @default(autoincrement()) @id
  email String
  password String
  first_name String
  last_name String
  token_version Int
}